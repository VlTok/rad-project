program Project1;

uses
  Vcl.Forms,
  Unit1 in 'Unit1.pas' {Employers},
  Unit2 in 'Unit2.pas' {Expenses};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TEmployers, Employers);
  Application.CreateForm(TExpenses, Expenses);
  Application.Run;
end.
